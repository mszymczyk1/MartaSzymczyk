
#include <iostream>

using namespace std;
const float CALE = 2.54;
const int STOPY = 12 * CALE;

int main()
{
    int wzrost_centymetry;
    cout << "Podaj wzrost w centymetrach:" << endl;
    cin >> wzrost_centymetry;
    cout << "Wzrost:" << wzrost_centymetry / STOPY << "_" << wzrost_centymetry / CALE - (wzrost_centymetry / STOPY) * 12 <<   endl;
    
}
