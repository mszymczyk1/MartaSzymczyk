

#include <iostream>

using namespace std;
double celsjusze_do_fahrenheitow(){
    double celsjusze;
    double fahrenheity;
    cout << "Podaj temperature w stopniach Celsjusza:";
    cin >> celsjusze;
    fahrenheity = 1.8 *celsjusze + 32;
    cout << celsjusze << " stopnie Celsjusza to " << fahrenheity << " stopnie Fahrenheita." << endl;
    
}
int main()
{
    celsjusze_do_fahrenheitow();
}
